{
    "name": "symfonyboot/inject-mocks",
    "description": "Automatic injection of mocks into test subjects via @InjectMocks and @Mock annotations, to speed up unit testing with PHPUnit.",
    "keywords": ["inject-mocks", "mocks", "tests", "annotations", "phpunit"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Yudi",
            "email": "symfonyboot@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": "^1.10",
        "symfonyboot/optional": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml.dist --testdox"
    },
    "autoload": {
        "psr-4": {
            "SymfonyBoot\\InjectMocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyBoot\\InjectMocks\\Tests\\": "tests/"
        }
    }
}
